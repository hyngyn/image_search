<!DOCTYPE html>

<html>
  <head>
    <title><%= title %></title>
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= javascript_include_tag "http://maps.googleapis.com/maps/api/js?libraries=places&sensor=false"%>
    <%= javascript_include_tag "jquery" %>
    <%= javascript_include_tag "jquery.masonry.min" %>
    <%= javascript_include_tag "jquery.infinitescroll.min" %>
    <%= javascript_include_tag "jquery.simplemodal-1.4.3" %>
    <%= stylesheet_link_tag 'custom', :media => 'screen' %>
    <%= csrf_meta_tags %>

  </head>
  

  <body>

    <div id="navigation_menu">
    </div>

    <%= yield %>

    <div id="footer">
    </div>

  </body>

	<%= debug(params) if Rails.env.development? %>

</html>
